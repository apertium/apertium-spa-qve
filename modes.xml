      </program>
    </pipeline>
  </mode>

  <mode name="qve-es-disam-trace">
    <pipeline>
      <program name="hfst-proc -w">
        <file name="qve-es.automorf.hfst"/>
      </program>
      <program name="cg-conv -a"/>
      <program name="vislcg3 --trace --grammar">
        <file name="qve-es.rlx.bin"/>
      </program>
    </pipeline>
  </mode>

